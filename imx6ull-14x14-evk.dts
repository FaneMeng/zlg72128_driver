	tsc2007@48 {
                compatible = "ti,tsc2007";
                reg = <0x48>;
                pinctrl-names = "default";
                pinctrl-0 = <&ts_int_pin>;
                interrupt-parent = <&gpio1>;
                interrupts = <9 0>;
                gpios = <&gpio1 9 GPIO_ACTIVE_LOW>;
                ti,x-plate-ohms = <660>;
                status = "disabled";
	};

	edt-ft5x06@38 {
                compatible = "edt,edt-ft5306", "edt,edt-ft5x06", "edt,edt-ft5406";
                pinctrl-names = "default";
                pinctrl-0 = <&ts_int_pin
                             &ts_reset_pin>;

                reg = <0x38>;
                interrupt-parent = <&gpio1>;
                interrupts = <9 0>;
                reset-gpios = <&gpio5 9 GPIO_ACTIVE_LOW>;
                irq-gpios = <&gpio1 9 GPIO_ACTIVE_LOW>;
                status = "disabled";
        };

	goodix_ts@5d{
                        compatible = "goodix,gt911";
                        reg = <0x5d>;
			status = "disabled";
                        interrupt-parent = <&gpio1>;
                        interrupts = <9 0>;
			pinctrl-0 = <&ts_int_pin
                             	     &ts_reset_pin>;
                        goodix_rst=<&gpio5 9 0>;
                        goodix_int=<&gpio1 9 0>;

        };

	sii902x: sii902x@39 {
                compatible = "SiI,sii902x";
                pinctrl-names = "default";
                pinctrl-0 = <&pinctrl_sii902x>;
                interrupt-parent = <&gpio5>;
                interrupts = <3 IRQ_TYPE_EDGE_FALLING>;
                irq-gpios = <&gpio5 3 GPIO_ACTIVE_LOW>;
                mode_str = "1280x720M@60";
                bits-per-pixel = <16>;
                resets = <&sii902x_reset>;
                reg = <0x39>;
                status = "disabled";
        };

	rtc@32{
        compatible = "ccteg,sd2506";
                reg = <0x32>;
		int_gpios = <&gpio4 25 GPIO_ACTIVE_LOW>;
        };
};
&i2c3{
	clock_frequency = <400000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3>;
	status = "okay";
	zlg72128_0: zlg72128_0@30 {
				compatible = "zlg72128";
				reg = <0x30>;
				i2cline = <3>;
				sla = <0x30>;
				irq = <96>;
				rst = <100>;
				keys_list = <
				 1 2 3 4 5 6 7 8
				 9 30 48 46 32 18 33 34
				 35 23 36 37 38 50 49 24
				 59 60 61 62 63 64 65 66
				>;
				 };
};
