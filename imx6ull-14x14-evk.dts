&ecspi2{
	fsl,spi-num-chipselects = <1>;
	cs-gpio = <&gpio1 23 GPIO_ACTIVE_LOW>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi2>;
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>; 

	spidev0:xfs5152ce@0 {               /*@后面的0表示此SPI芯片接到哪个硬件片选上*/
		compatible = "ccteg,xfs5152ce";
		spi-max-frequency = <20000>;
		reg = <0>;
	};
};

&i2c2 {
	clock_frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";

	codec: wm8960@1a {
		compatible = "wlf,wm8960";
		reg = <0x1a>;
		clocks = <&clks IMX6UL_CLK_SAI2>;
		clock-names = "mclk";
		wlf,shared-lrclk;
	};

